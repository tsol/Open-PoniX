	  if test -f "$$p"; then d=; else d="$(srcdir)/"; fi; \
	  f=$(am__strip_dir) \
	  echo " $(configexecincludeDATA_INSTALL) '$$d$$p' '$(DESTDIR)$(configexecincludedir)/$$f'"; \
	  $(configexecincludeDATA_INSTALL) "$$d$$p" "$(DESTDIR)$(configexecincludedir)/$$f"; \
	done

