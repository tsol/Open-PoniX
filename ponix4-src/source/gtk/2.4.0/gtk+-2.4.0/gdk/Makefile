	  if test -f "$$p"; then d=; else d="$(srcdir)/"; fi; \
	  f="`echo $$p | sed -e 's|^.*/||'`"; \
	  echo " $(gdkincludeHEADERS_INSTALL) $$d$$p $(DESTDIR)$(gdkincludedir)/$$f"; \
	  $(gdkincludeHEADERS_INSTALL) $$d$$p $(DESTDIR)$(gdkincludedir)/$$f; \
	done

