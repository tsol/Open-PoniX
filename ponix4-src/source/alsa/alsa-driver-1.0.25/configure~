PACKAGE_VERSION=
PACKAGE_STRING=
PACKAGE_BUGREPORT=
PACKAGE_URL=

ac_unique_file="acore/sound.patch"
ac_default_prefix=/usr
ac_subst_vars='LTLIBOBJS
LIBOBJS
