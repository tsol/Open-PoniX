mkdir -p include/trace
rm -f include/trace/events
ln -sf ../../alsa-kernel/trace_events include/trace/events


  { $as_echo "$as_me:${as_lineno-$LINENO}: checking for kernel linux/tracepoint.h" >&5
$as_echo_n "checking for kernel linux/tracepoint.h... " >&6; }
  if test -n "$kernelbuild" -a \( -f "$kernelbuild/include/linux/tracepoint.h" -o -f "$kernelbuild/include2/linux/tracepoint.h" -o -f "$kernelbuild/arch/$ARCHDIR/include/linux/tracepoint.h" \) ; then
