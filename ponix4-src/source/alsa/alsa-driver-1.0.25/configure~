      mkdir -p include/linux include/asm include/media
      mkdir -p include/linux/regulator include/linux/usb include/pcmcia
      mkdir -p include/trace
      if test -z "" ; then
        echo "Creating a dummy <media/v4l2-ctrls.h>..."
        touch include/media/v4l2-ctrls.h
      else
        echo "Creating <media/v4l2-ctrls.h>..."
        echo "" > include/media/v4l2-ctrls.h
