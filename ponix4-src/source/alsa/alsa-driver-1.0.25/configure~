    CONFIG_SND_DEBUG_MEMORY=y
    $as_echo "#define CONFIG_SND_DEBUG_MEMORY 1" >>confdefs.h

    CONFIG_SND_DEBUG_VERBOSE=y
    $as_echo "#define CONFIG_SND_DEBUG_VERBOSE 1" >>confdefs.h

    ;;
  *)
    { $as_echo "$as_me:${as_lineno-$LINENO}: result: none" >&5
$as_echo "none" >&6; }
    ;;
esac

