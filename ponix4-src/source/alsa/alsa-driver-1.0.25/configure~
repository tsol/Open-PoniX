      if test -z "#include \"bitdev_compat.h\"
" ; then
        echo "Creating a dummy <linux/bitrev.h>..."
        touch include/linux/bitrev.h
      else
        echo "Creating <linux/bitrev.h>..."
        echo "#include \"bitdev_compat.h\"
" > include/linux/bitrev.h
      fi
    fi
  fi


  { $as_echo "$as_me:${as_lineno-$LINENO}: checking for kernel linux/smp_lock.h" >&5
$as_echo_n "checking for kernel linux/smp_lock.h... " >&6; }
  if test -n "$kernelbuild" -a \( -f "$kernelbuild/include/linux/smp_lock.h" -o -f "$kernelbuild/include2/linux/smp_lock.h" -o -f "$kernelbuild/arch/$ARCHDIR/include/linux/smp_lock.h" \) ; then
    KERNDIR=$kernelbuild
  else
    KERNDIR=$CONFIG_SND_KERNELDIR
  fi
  if test -n ""; then
    eval =""
  fi
  if test -f "$KERNDIR/include/linux/smp_lock.h" -o -f "$KERNDIR/include2/linux/smp_lock.h" -o -f "$KERNDIR/arch/$ARCHDIR/include/linux/smp_lock.h"; then
    { $as_echo "$as_me:${as_lineno-$LINENO}: result: yes" >&5
$as_echo "yes" >&6; }
    if test -f include/linux/smp_lock.h; then
      echo "Removing a dummy linux/smp_lock.h."
      rm -f include/linux/smp_lock.h
    fi
    if test -n ""; then
      eval ="y"
    fi
  else
    { $as_echo "$as_me:${as_lineno-$LINENO}: result: no" >&5
$as_echo "no" >&6; }
    if test ! -f include/linux/smp_lock.h; then
      mkdir -p include/linux include/asm include/media
      mkdir -p include/linux/regulator include/linux/usb include/pcmcia
      mkdir -p include/trace
      if test -z "" ; then
        echo "Creating a dummy <linux/smp_lock.h>..."
        touch include/linux/smp_lock.h
      else
        echo "Creating <linux/smp_lock.h>..."
        echo "" > include/linux/smp_lock.h
      fi
    fi
  fi


  { $as_echo "$as_me:${as_lineno-$LINENO}: checking for kernel linux/i8253.h" >&5
$as_echo_n "checking for kernel linux/i8253.h... " >&6; }
